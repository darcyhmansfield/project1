<%= form_for @promotion do |f| %>
    <fieldset>
        <%= f.label :abbrev, "Abbreviation" %>
        <%= f.text_field :abbrev %>

        <%= f.label :logo %>
        <%= f.url_field :logo %>

        <%= f.label :name %>
        <%= f.text_field :name %>

        <%= f.label :country %>
        <%= f.text_field :country %>

        <%= f.label :year %>
        <%= f.number_field :year %>

        <%= f.label :instagram %>
        <%= f.url_field :instagram %>

        <%= f.submit %>

    </fieldset>
<% end %>